{
  "type-check":[],
  "rules": {
    "max-line-length": {
      "options": [500]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "experimentalDecorators": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": {
      "options": [
        "debug",
        "info",
        "time",
        "timeEnd",
        "trace"
      ]
    }
  },
  "jsRules": {
    "max-line-length": {
      "options": [200]
    }
  }
}
